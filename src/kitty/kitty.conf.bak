font_family      MartianMono Nerd Font
bold_font        MartianMono Nerd Font
italic_font      MartianMono Nerd Font
bold_italic_font MartianMono Nerd Font

font_size 16.0

input_delay 0
repaint_delay 2
sync_to_monitor no
wayland_enable_ime no

enable_audio_bell no
window_border_width 0
draw_minimal_borders no
update_check_interval 0

# BEGIN_KITTY_THEME
# Earthsong
include current-theme.conf
# END_KITTY_THEME
