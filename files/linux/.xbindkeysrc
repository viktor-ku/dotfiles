# Increase Volume
"pactl set-sink-volume @DEFAULT_SINK@ +2.5%"
  Mod4 + F12

# Decrease Volume
"pactl set-sink-volume @DEFAULT_SINK@ -2.5%"
  Mod4 + F11

# Mute Volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
  Mod4 + F10

"playerctl play-pause"
  Mod4 + F8

"playerctl next"
  Mod4 + F9

"playerctl previous"
  Mod4 + F7

