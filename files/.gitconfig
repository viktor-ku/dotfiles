[user]
  name = Viktor Kuroljov
  email = viktor@ku.family
[core]
  editor = vim
  whitespace = trailing-space,space-before-tab
[alias]
  aa = add --all
  au = add -u
  s = status
  ss = status -s
  ch = checkout
  cm = commit
  b = branch
  sm = submodule
  lg = log --oneline --all --decorate --graph
  pp = push
  diffc = diff --cached
  pum = push --set-upstream origin master
[push]
  default = simple
[color]
  ui = auto
  interactive = auto
[rerere]
  enabled = true
